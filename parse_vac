import requests

# задаем параметры
url = 'https://api.hh.ru/vacancies'
text = "Python"  # text: language (job)
number_of_pages = 10
per_page = 20    # кол-во на странице

# предварительный список, равный кол-ву страниц
pre_res = []

for page in range(number_of_pages):
    
    par = {'text': text,'per_page': per_page, 'page':page}
    resp = requests.get(url, params = par)
    cle = resp.json()
    pre_res.append(cle)

# создадим один общий список
res = []

for el in pre_res: # el - dict
    for pg in el:  # pg - одна найденная страница
        res.append(pg) 
# тут не то - собрало только список ключей
